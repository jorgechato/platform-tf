[
  {
    "name": "${container_name}",
    "image": "${image}",
    "memory": 128,
    "memoryReservation": 64,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 5000,
        "hostPort": 5000
      }
    ],
    "environment": [
      {
        "name": "POLARSTEP_USER",
        "value": "${polarstep_user}"
      }
    ]
  }
]
