[
  {
    "name": "${container_name}",
    "image": "${image}",
    "memoryReservation": 128,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 5000,
        "hostPort": 5000
      }
    ],
    "environment": [
      {
        "name": "GITHUB_TOPIC",
        "value": "${github_topic}"
      },
      {
        "name": "GITHUB_USER",
        "value": "${github_user}"
      },
      {
        "name": "GITHUB_TOKEN",
        "value": "${github_token}"
      },
      {
        "name": "GIST_LIST",
        "value": "${gist_list}"
      },
      {
        "name": "MAILCHIMP_DS",
        "value": "${mailchimp_ds}"
      },
      {
        "name": "MAILCHIMP_KEY",
        "value": "${mailchimp_key}"
      },
      {
        "name": "MAILCHIMP_LIST",
        "value": "${mailchimp_list}"
      },
      {
        "name": "LOCATION_USER",
        "value": "${location_user}"
      }
    ]
  }
]
