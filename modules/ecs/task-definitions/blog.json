[
  {
    "name": "${container_name}",
    "image": "${image}",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 2368,
        "hostPort": 80
      }
    ],
    "environment": [
      {
        "name": "email",
        "value": "${email}"
      },
      {
        "name": "url",
        "value": "${url}"
      }
    ]
  }
]

